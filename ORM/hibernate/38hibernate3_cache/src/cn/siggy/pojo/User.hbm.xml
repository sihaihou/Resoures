<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.siggy.pojo.User">
	<cache usage="read-only"/>
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="name"/>
		<property name="pwd"/>
	</class>
</hibernate-mapping>